#include <iostream>
#include <vector>

using namespace std;

int main()
{
	int n,tem;
	cin >> n;
	vector<int> nums(n);
	for (int i = 0; i < n; i++) {
		cin >> nums[i];
	}
	for (int j = 0;j < n - 1;j++)
	{
		for (int i = 0; i < n; i++) {
			if (i == n - 1 - j)
				break;
			if (nums[i] > nums[i + 1]) {
				tem = nums[i];
				nums[i] = nums[i + 1];
				nums[i + 1] = tem;
			}
		}
	}
	do {
		n--;
		cout << nums[n] << endl;
	} while (n > 0);
	return 0;
}
