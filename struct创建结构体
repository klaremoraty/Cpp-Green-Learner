#include "swap.h"
#include <iostream>
using namespace std;

struct Student {
	string name;
	int age;
	int score;
}s3;

int main()
{
	struct Student s1;
	s1.name = "Lisa";
	s1.age = 18;
	s1.score = 100;

	struct Student s2 = { "Bob",19,99 };
	s3.name = "Kate";
	s3.age = 20;
	s3.score = 90;
	cout << "name: " << s3.name << endl;
	cout << "age: " << s3.age << endl;
	cout << "score: " << s3.score << endl;
	return 0;
}

